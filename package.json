{
    "name": "qacart-todo_cypress",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "cypress": "^12.14.0",
        "json-server": "^0.16.3",
        "npm-run-all": "^4.1.5",
        "react": "^15.5.4",
        "react-dom": "^15.5.4"
    },
    "devDependencies": {
        "json-server-auth": "^2.0.2",
        "react-scripts": "0.9.5",
        "start-server-and-test": "^2.0.0"
    },
    "scripts": {
        "start:frontend": "react-scripts start",
        "start:server": "json-server -p 8080 db.json -m ./node_modules/json-server-auth -r routes.json",
        "start": "npm-run-all --parallel start:server start:frontend",
        "cyrun": "npx cypress run --record --key 8a918215-83b6-4927-916e-f3a5ac00fb76",
        "build:and:test": "start-server-and-test start http://localhost.3000 cyrun",
        "build": "react-scripts build",
        "test": "react-scripts test --env=jsdom",
        "eject": "react-scripts eject",
        "e22:chrome": "cypress run --browser chrome --headless",
        "e22:firefox": "cypress run --browser firefox --headless"
    }
}
